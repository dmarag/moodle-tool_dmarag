{{!
    @template mod_questionnaire/indexpage

    Template which defines a questionnaire index page.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * headings - array: An array of table headings.
    * rows - array: An array of content rows for the table.

    Example context (json):
    {
        "headings": [
            {
                "title1": "Id",
                "title2": "Name",
				"title3": "Description",
                "title4": "Completed",
				"title5": "Priority",
				"title6": "Timecreated",
				"title7": "Timemodified",
				"title8": "Delete"
            }
        ]
    },
    {
        "rows": [
            {
                "id": "1",
                "name": "test",
				"description": "",
                "completed": "",
				"priority": "",
				"timecreated": "",
				"timemodified": "",
				"delete": "",
            }
        ]
    }
    }}
	
	
       
			
<table class="generaltable">
  <thead>
    <tr>
{{#headings}}
      <th class="header" style="text-align:left;" scope="col">{{title1}}</th>
      <th class="header" style="text-align:left;" scope="col">{{title2}}</th>
      <th class="header" style="text-align:left;" scope="col">{{title3}}</th>
      <th class="header" style="text-align:left;" scope="col">{{title4}}</th> 
	  <th class="header" style="text-align:left;" scope="col">{{title5}}</th>
      <th class="header" style="text-align:left;" scope="col">{{title6}}</th>
      <th class="header" style="text-align:left;" scope="col">{{title7}}</th>
	  <th class="header" style="text-align:left;" scope="col">{{title8}}</th>
{{/headings}}
    </tr>
  </thead>
  <tbody>
{{#rows}}
    <tr>
      <td class="cell" style="text-align:left;" scope="col">{{id}}</td>
      <td class="cell" style="text-align:left;" scope="col">{{{name}}}</td>
	  <td class="cell" style="text-align:left;" scope="col">{{{description}}}</td>
      <td class="cell" style="text-align:center;" scope="col">{{completed}}</td>
	  <td class="cell" style="text-align:center;" scope="col">{{priority}}</td>
	  <td class="cell" style="text-align:left;" scope="col">{{{timecreated}}}</td>
	  <td class="cell" style="text-align:left;" scope="col">{{{timemodified}}}</td>
	  <td class="cell" style="text-align:center;" scope="col">{{{delete}}}</td>
    </tr>
{{/rows}}
  </tbody>
</table>